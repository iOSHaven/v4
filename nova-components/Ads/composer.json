{
    "name": "ioshaven/ads",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ioshaven\\Ads\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ioshaven\\Ads\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
