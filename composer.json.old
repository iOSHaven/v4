{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/v4"
        },
        {
            "type": "path",
            "url": "./nova-components/Ads"
        },
        {
            "type": "path",
            "url": "./nova-components/plist"
        }
    ],
    "require": {

        "ext-bcmath": "^8.1.0",
        "anhskohbo/no-captcha": "^3.3",
        "david-griffiths/nova-dark-theme": "^1.0",
        "doctrine/dbal": "^2.9",
        "eminiarts/nova-tabs": "^1.4",
        "erusev/parsedown": "^1.7",
        "fedeisas/laravel-mail-css-inliner": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inspheric/nova-url-field": "^1.4",
        "ioshaven/ads": "dev-master",
        "ioshaven/plist": "dev-master",
        "ioshaven/v4": "dev-master",
        "jdenticon/jdenticon": "^0.10.0",
        "jenssegers/agent": "^2.6",
        "kabbouchi/nova-impersonate": "^1.10",
        "laravel/cashier": "^13.6",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.9",
        "laravel/nova": "^3.22",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "marvinlabs/laravel-discord-logger": "^1.1",
        "moontoast/math": "^1.2",
        "paypal/rest-api-sdk-php": "^1.14",
        "pda/pheanstalk": "~4.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "saumini/count": "^1.0",
        "spatie/laravel-sitemap": "^5.8",
        "symfony/translation-contracts": "^2.5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "davidbadura/faker-markdown-generator": "^1.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^2.5",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
